  "label": "Event",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "unique identifier",
          "placeholder": "slug-url"
        }
      },
      "title": {
        "type": "StructuredText",
