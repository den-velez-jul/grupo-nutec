      "start_day": {
        "type": "Date",
        "config": {
          "label": "Start Day"
        }
      },
      "country": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph,strong",
          "label": "Country",
          "placeholder": "USA / MEXICO"
