        "type": "Text",
        "config": {
          "label": "meta keyword"
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "meta title"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "meta description"
        }
      },
      "image1": {
        "type": "Image",
        "config": {
          "constraint": {
            "width": 512,
            "height": 512
