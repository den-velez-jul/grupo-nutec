{
  "id": "career",
  "label": "Career",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": { "type": "UID", "config": { "label": "unique identifier" } },
      "title": {
        "type": "StructuredText",
        "config": { "single": "heading1", "label": "Title" }
      },
      "title_for_feed": {
        "type": "StructuredText",
        "config": { "single": "heading2", "label": "Title for feed" }
      },
      "category": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph",
          "allowTargetBlank": true,
          "label": "Category"
        }
      },
      "location": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph",
          "allowTargetBlank": true,
          "label": "Location"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,heading4,strong,em,hyperlink,list-item",
          "allowTargetBlank": true,
          "label": "Description"
        }
      },
      "full_description": {
        "type": "StructuredText",
        "config": {
          "multi": "paragraph,heading4,strong,o-list-item",
          "allowTargetBlank": true,
          "label": "Full Description",
          "placeholder": "Complete job application info"
        }
      },
      "career_cta": {
        "type": "Link",
        "config": {
          "label": "career cta",
          "placeholder": "",
          "allowTargetBlank": false,
          "select": null
        }
      },
      "career_cta_label": {
        "type": "StructuredText",
        "config": {
          "label": "career cta label",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      }
    },
    "SEO": {
      "meta_keyword": { "type": "Text", "config": { "label": "Meta keyword" } },
      "meta_title": { "type": "Text", "config": { "label": "meta title" } },
      "meta_description": {
        "type": "Text",
        "config": { "label": "Meta description" }
      }
    }
  }
}
